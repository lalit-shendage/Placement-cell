<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>studentlist </title>
    <link rel="stylesheet" href="/static/css/style.css">
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp"
      crossorigin="anonymous"
    />
</head>
<body>
    <%- include('navbar') %>
    <div class="container">
    <h3 class="align-content-center my-4"><%= student.name %></h3>
    <ul class="list-group">
        <li class="list-group-item ">College: <%= student.college %></li>
        <li class="list-group-item"> Batch:<%= student.batch %></li>
        <li class="list-group-item">Placement Status: <%= student.status %></li>
        <li class="list-group-item">Web Development score:<%= student.webDScore %></li>
        <li class="list-group-item">React score: <%= student.reactScore %></li>
        <li class="list-group-item">DSA Score:<%= student.dsaScore %></li>
      </ul>
      <h3 class="my-3"> Interview List</h3>
      <ul class="list-group">
        <% for(let company of student.interviews){%>
        <li><a href=" " id="compnay_name"><%=company.company%></a></li>
        <%}%>
    </ul>
    <!-- <button class="btn btn-primary my-3">
        <a class="nav-link text-light" href="/company/addinterview/<%=student.id%>">shedule interview</a>
      </button> -->
</div>
<script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N"
      crossorigin="anonymous"
    ></script>
</body>
</html>